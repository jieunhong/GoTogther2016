<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="weourus.emirim.hs.kr.gotogether">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:windowSoftInputMode="adjustPan"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityTravelAdd"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".ActivityTravelList"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".ActivityLocationAddAutoComplete"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".ActivityLocationAdd"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".ActivityLocationList"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".ActivityTravelDetail"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".ActivityTravelEdit"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>